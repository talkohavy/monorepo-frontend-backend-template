{
  "name": "@root-app/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules",
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "vite-optimize": "vite optimize --force",
    "lint": "eslint -c .eslintrc.json '**/*/*.{js,jsx,tsx,ts,d.ts,cjs,mjs}'",
    "lint-fix": "eslint --fix -c .eslintrc.cjs '**/*/*.{js,jsx,tsx,ts,d.ts,cjs,mjs}'",
    "format": "biome format --verbose ./src",
    "format-prettier": "prettier . --check --config ../../.prettierrc.mjs --ignore-path ../../.prettierignore",
    "format-fix": "prettier . --write --config ../../.prettierrc.mjs --ignore-path ../../.prettierignore",
    "typecheck": "tsc -p jsconfig.json",
    "test": "node --test"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.6.0",
    "clsx": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/parser": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0"
  }
}
