{
  "name": "@root-app/frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf node_modules",
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "vite-optimize": "vite optimize --force",
    "lint": "eslint -c .eslintrc.cjs '**/*/*.{js,jsx,tsx,ts,d.ts,cjs,mjs}'",
    "lint-fix": "eslint --fix -c .eslintrc.cjs '**/*/*.{js,jsx,tsx,ts,d.ts,cjs,mjs}'",
    "format": "biome format --verbose ./src",
    "format-prettier": "prettier . --check --config ../../.prettierrc.mjs --ignore-path ../../.prettierignore",
    "format-fix": "prettier . --write --config ../../.prettierrc.mjs --ignore-path ../../.prettierignore",
    "typecheck": "tsc -p jsconfig.json",
    "test": "node --test"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.6.0",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ezlint": "^1.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.6"
  }
}
